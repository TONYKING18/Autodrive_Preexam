//python//
def Fizz():
    for i in range(1, 26):  # Loop from 1 to 25
        output = ""
        if i % 3 == 0:
            output += "Fizz"
        if i % 5 == 0:
            output += "Fizzy"
        if i % 7 == 0:
            output += "Buzz"
        if output == "":
            output = str(i)  # If not divisible by 3, 5, or 7, just output the number
        print(output, end=", " if i < 25 else "\n")  

Fizz()
